[core]
  editor = /usr/bin/vim
  excludesfile = ~/.gitignore
[color]
  ui = auto
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  whitespace = trailing-space,space-before-tab
[gist]
  private = yes
[push]
  default = simple
[user]
	email = brian.hartsock@gmail.com
	name = Brian Hartsock
